{
  "projectType": "library",
  "sourceRoot": "libs/emote-list/src",
  "prefix": "nx-twitch",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/emote-list"],
      "options": {
        "project": "libs/emote-list/ng-package.json",
        "tailwindConfig": "libs/emote-list/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/emote-list/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/emote-list/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/emote-list"],
      "options": {
        "jestConfig": "libs/emote-list/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/emote-list/**/*.ts",
          "libs/emote-list/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
